http {
    upstream backend {
        server prx1proton.herokuapp.com weight=3;
        # server 0.0.0.0:5001;
		# server 0.0.0.0:5002;
		# server 0.0.0.0:5003;
    }

	server {
		listen <%= ENV["PORT"] %>;
		#server_name _;

		location / {
			proxy_pass http://backend;
            #proxy_redirect off;
			#proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
			#proxy_set_header Host $http_host;
		}
	}
}